{
  "builders": [
    {
      "name": "gcp_fabric",
      "type": "docker",
      "image": "debian",
      "export_path": "image.tar"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": [
        "apt-get -y update",
        "apt-get -y install python-pip python-dev python-flask python-wtforms python-arrow python-flask-sqlalchemy python-pymysql python-flaskext.wtf"
      ]
    }
  ],
  "post-processors": [
    {
      "type": "docker-import",
      "repository": "debian",
      "tag": "latest"
    }
  ]
}
